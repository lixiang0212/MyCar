<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:id="@+id/inflate_b_linear"
    android:background="@color/colorWhite"
    android:layout_height="@dimen/xp193">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/xp192">
    <ImageView
        android:id="@+id/inflate_b_iv"
        android:layout_width="@dimen/xp192"
        android:layout_marginTop="@dimen/xp20"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/xp20"
        />
    <TextView
        android:textColor="#3C3C3C"
        android:id="@+id/inflate_b_tv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:textSize="@dimen/xp36"
        android:gravity="center_vertical"
        android:layout_marginLeft="@dimen/xp30"
        android:text="name"
        />
    </LinearLayout>
    <View
        android:layout_marginLeft="@dimen/xp40"
        android:layout_marginRight="@dimen/xp40"
        android:background="@color/colorGro"
        android:layout_width="match_parent"
        android:layout_height="@dimen/xp1"/>

</LinearLayout>